m_cnt = 1;
for m = ModeList
    s_cnt = 1;
    for s=SubList
            load(['/Users/sorooshafyouni/Home/DVARS/fMRIDiag/HCP/DSE/R/' m{1} '/HCP_DSE_' m{1} '_' s{1} '_' GSRStat{1} '.mat'],'DSE_Stat','V')
%------------------------------------------------------------------
            pD(s_cnt,m_cnt)=DSE_Stat.Prntg(2);
            pS(s_cnt,m_cnt)=DSE_Stat.Prntg(3);
            pE(s_cnt,m_cnt)=DSE_Stat.Prntg(3);
            
            pgD(s_cnt,m_cnt)=DSE_Stat.Prntg(2);
            pgS(s_cnt,m_cnt)=DSE_Stat.Prntg(3);
            pgE(s_cnt,m_cnt)=DSE_Stat.Prntg(3);
%             pngD(s_cnt,m_cnt)=DSE_Stat.Prntg(2);
%             pngS(s_cnt,m_cnt)=DSE_Stat.Prntg(3);
%------------------------------------------------------------------
            mD(s_cnt,m_cnt)=sqrt(mean(V.Dvar_ts));
            mS(s_cnt,m_cnt)=sqrt(mean(V.Svar_ts));
            
            mgD(s_cnt,m_cnt)=sqrt(mean(V.Dvar_ts));
            mgS(s_cnt,m_cnt)=sqrt(mean(V.Svar_ts));
            
%             mngD(s_cnt,m_cnt)=sqrt(mean(V.Dvar_ts));
%             mngS(s_cnt,m_cnt)=sqrt(mean(V.Svar_ts));
            
            s_cnt = s_cnt + 1;
    end
        m_cnt = m_cnt+1;
end

